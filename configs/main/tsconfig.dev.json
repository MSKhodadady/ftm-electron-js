{
    "compilerOptions": {
        "module": "CommonJS",
        "inlineSourceMap": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "outDir": "../../dist",
        "target": "ES2020",
        "types": ["node"]
    },
    "include": ["../../src/main/**/*", "../../src/common/**/*"],
    "exclude": ["../../node_modules"],
    "typeAcquisition": {
        "enable": false
    }
}